#include "holberton.h"

/**
 * times_table - check the code for Holberton School students.
 *
 * Return: Always 0.
 */

void times_table(void)
{
	int x, y, z = 0;

			for (x = 0; x < 10; x++)
		{

				for (y = 0; y < 10; y++)
				{
					z = x * y;

					if (z < 10)
						{
							_putchar(z + '0');
							_putchar(',');
							_putchar(' ');
							_putchar(' ');
					        }
					else
						{


							_putchar((z / 10)+'0');
							_putchar((z % 10)+'0');
							_putchar(',');
							_putchar(' ');

						}

					if (y == 9)
						_putchar(z);
				}
			_putchar('\n');
		}
}
